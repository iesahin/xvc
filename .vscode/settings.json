{
    "grammarly.selectors": [
        {
            "language": "markdown",
            "scheme": "file"
        }
    ],
    "grammarly.files.exclude": [
        "**/CHANGELOG.*"
    ],
    "grammarly.files.include": [
        "**/readme.md",
        "**/README.md",
        "**/*.txt",
        "book/**/*.md"
    ],
    "grammarly.config.documentDialect": "american",
    "grammarly.config.suggestions.ConjunctionAtStartOfSentence": true,
    "grammarly.config.suggestions.OxfordComma": true,
    "grammarly.config.suggestions.PassiveVoice": true,
    "grammarly.config.suggestions.PrepositionAtTheEndOfSentence": true,
    "grammarly.config.suggestions.StylisticFragments": true,
    "grammarly.config.suggestions.UnnecessaryEllipses": true,
    "rust-analyzer.linkedProjects": [
        "./pipeline/Cargo.toml"
    ],
    "workbench.colorCustomizations": {
        "titleBar.activeBackground": "#a6e559",
        "titleBar.activeForeground": "#000000",
        "titleBar.inactiveBackground": "#a6e559",
        "titleBar.inactiveForeground": "#000000",
        "titleBar.border": "#a6e559",
        "activityBar.background": "#a6e559",
        "activityBar.foreground": "#000000",
        "statusBar.background": "#a6e559",
        "statusBar.foreground": "#000000",
        "statusBar.debuggingBackground": "#a6e559",
        "statusBar.debuggingForeground": "#000000",
        "tab.activeBorder": "#a6e559",
        "iLoveWorkSpaceColors": true,
        "iLoveWorkSpaceRandom": true
    }
}
